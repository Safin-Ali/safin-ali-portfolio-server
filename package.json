{
	"name": "safin-ali-portfolio-server",
	"version": "1.0.0",
	"description": "basic express app boilerplate",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"build": "clear && babel src --out-dir dist --extensions \".ts\"",
