		"expressjs",
		"mongoose",
		"mongodb",
		"dot-env",
		"jwt"
	],
	"author": "Safin Ali",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Safin-Ali/safin-ali-portfolio-server"
	},
	"homepage": "https://github.com/Safin-Ali/safin-ali-portfolio-server#readme",
	"dependencies": {
		"core-js": "^3.32.1",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"mongoose": "^7.4.1",
		"nodemailer": "^6.9.5"
	},
	"devDependencies": {
