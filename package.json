		"@types/express": "^4.17.18",
		"@types/nodemailer": "^6.4.11",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"babel-plugin-tsconfig-alias-to-relative": "^1.0.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0"
